{
  "definitions": {
    "Enemy": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "BaseId": {
          "type": "integer"
        },
        "Attachments": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        }
      },
      "required": [
        "BaseId",
        "Attachments"
      ]
    },

    "EnemySet": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Enemy": {
          "$ref": "#/definitions/Enemy"
        },
        "TotalKills": {
          "type": "integer"
        }
      },
      "required": [
        "Enemy",
        "TotalKills"
      ]
    },

    "Impulse": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Id": {
          "type": "integer"
        },
        "Count": {
          "type": "integer"
        }
      },
      "required": [
        "Id",
        "Count"
      ]
    },

    "MedalAward": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "MedalId": {
          "type": "integer"
        },
        "Count": {
          "type": "integer"
        }
      },
      "required": [
        "MedalId",
        "Count"
      ]
    },

    "WeaponId": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "StockId": {
          "type": "integer"
        },
        "Attachments": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        }
      },
      "required": [
        "StockId",
        "Attachments"
      ]
    },

    "WeaponStat": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "WeaponId": {
          "$ref": "#/definitions/WeaponId"
        },
        "TotalShotsFired": {
          "type": "integer"
        },
        "TotalShotsLanded": {
          "type": "integer"
        },
        "TotalHeadshots": {
          "type": "integer"
        },
        "TotalKills": {
          "type": "integer"
        },
        "TotalDamageDealt": {
          "type": "number"
        },
        "TotalPossessionTime": {
          "type": "string"
        }
      },
      "required": [
        "WeaponId",
        "TotalShotsFired",
        "TotalShotsLanded",
        "TotalHeadshots",
        "TotalKills",
        "TotalDamageDealt",
        "TotalPossessionTime"
      ]
    }
  }
}